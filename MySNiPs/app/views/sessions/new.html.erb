<h1>Log In</h1>
<%= form_for :login do |f| %>
    <div>
        <%= f.label :identifier %>
        <%= f.text_field :identifier, autofocus: true %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    <div>
        <%= f.submit "Log In" %>
    </div>
<% end %>