<div class="page-header">
  <h1>Report</h1>
  <small>#<%= current_user.identifier %></small>
</div>

<%= form_tag(report_index_url, method: :get) do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search' %>
<% end %>


<div id="cards">
  <%= render @cards %>
</div>

<div id="infinite-scrolling">
  <%= will_paginate @cards %>
</div>
